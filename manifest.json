{
  "manifest_version": 2,

  "name": "LinkedIn to Salesforce",
  "description": "This extension pumps people from linkedin to Salesforce",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "LinkedIn To Salesforce"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}